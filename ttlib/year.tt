[% WRAPPER page.tt -%]
      <h1 class="display-4 fst-italic">Years</h1>
      <ul>
[% FOR y IN years -%]
        <h2>[% y.year %]</h2>
        <ul>
[%   FOR e IN y.events -%]
          <li><a href="[% e.url_path %]">[% e.name_year %], [% e.location %]</a></li>
[%   END -%]
        </ul>
[% END -%]
      </ul>
[% END -%]
