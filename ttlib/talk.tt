[% WRAPPER page.tt -%]
      <h1 class="display-4 fst-italic">[% talk.title %]</h1>
      <p><span class="badge rounded-pill bg-primary">[% talk.talk_type.type %]</span></p>
      <h2>Presentations</h2>
      <ul>
[% FOR p IN talk.presentations -%]
        <li>[% p.event.description %] - [% p.datetime %]</li>
[% END -%]
      </ul>
[% END -%]
